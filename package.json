{
  "name": "fpho",
  "version": "0.0.0",
  "private": true,
  "description": "Flux Price History Oracle - minute-grade FMV with hourly Flux anchors",
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "lint": "pnpm -r lint && eslint .",
    "format": "prettier --check .",
    "test": "pnpm -r test",
    "build": "pnpm -r build",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:reset": "tsx scripts/db-reset.ts",
    "simulate:hour": "tsx scripts/simulate_hour.ts",
    "simulate:quorum": "tsx scripts/reporter_sim.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "better-sqlite3": "^11.8.1",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.5"
  }
}
