[Unit]
Description=FPHO Quorum Signer (dev-mode)
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
WorkingDirectory=/root/Flux-Oracle
EnvironmentFile=-/etc/fpho/fpho.env
ExecStart=/usr/bin/node /root/Flux-Oracle/deploy/vps/fpho-signer.mjs
Restart=on-failure
RestartSec=2

[Install]
WantedBy=multi-user.target

